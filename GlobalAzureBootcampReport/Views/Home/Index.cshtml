@model IEnumerable<GlobalAzureBootcampReport.Models.UserStat>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title></title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.React("Components.App", new {initialStats =  Model}, containerId: "app")

    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/react/0.13.1/react-with-addons.min.js"></script>
    @Scripts.Render("~/bundles/scripts")
    <script src="@Url.Content("~/Scripts/build/client.bundle.js")"></script>
    @Html.ReactInitJavaScript()
</body>
</html>